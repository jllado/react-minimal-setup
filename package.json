{
  "name": "react-setup",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.babel.js",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules"],
    "moduleNameMapper": {
      "\\.(eot|woff|woff2|ttf|svg|png|jpg|jpeg|gif)$": "<rootDir>/config/jest/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "/test/.*\\.js$"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.5.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
